{
	"api": {
		"adventSociety": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "API Key",
							"apiKeyExpirationDays": "365"
						}
					}
				}
			}
		}
	},
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"getPuzzlesForDay": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "adventSociety",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		},
		"puzzleComplete": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "adventSociety",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		}
	}
}